#!@BASH@

# Ignore this test if no STM

if [ -z "@NEBELUNG_INCLUDES@" ];
then

cat  <<EOF
test_ignore=yes
EOF

fi

source @abs_builddir@/mercurium-libraries

cat <<EOF
MCC="@abs_top_builddir@/src/driver/mcxx --output-dir=@abs_top_builddir@/tests --profile=stmcxx --config-file=@abs_top_builddir@/config/config.stm_nebelung --verbose"

test_CXX="\${MCC}"

test_CXXFLAGS=

test_nolink=yes
EOF
